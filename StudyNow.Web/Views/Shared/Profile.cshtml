                        </svg>
                    </div>
                    <div class="profile--body-general-info-list-item-info">
                        <h5>Тип користувача</h5>
                        <p>@(Model.UserType == UserType.Student ? "Студент" : "Адміністратор")</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="profile--body-settings">
            <h2>Налаштування</h2>
            <div class="profile--body-settings-buttons">
                <form asp-action="DeleteAccount" method="post" onsubmit="return confirm('Ви впевнені, що хочете видалити свій акаунт?');">
                    <button type="submit" class="button button-dark">Видалити аккаунт</button>
                </form>
                <form asp-controller="Authorization" asp-action="Logout" method="post">
                    <button type="submit" class="button button-red">Вийти</button>
                </form>
            </div>
        </div>
    </div>
</div>
