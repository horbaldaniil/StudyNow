                    </div>
                    <div class="profile--body-general-info-list-item-info">
                        <h5>Група</h5>
                        <p>@Model.GroupName</p>
                    </div>
                </div>
                <div class="profile--body-general-info-list-item">
                    <div class="profile--body-general-info-list-item-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="800px" height="800px" viewBox="0 0 24 24" fill="none">
